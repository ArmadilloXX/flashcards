<div class="panel panel-default", id="panel">
  <div class="panel-heading">
    <h3 class="panel-title">We found some photos for "<%= "#{@search_term}" %>" </h3>
  </div>
  <div class="panel-body">
    <div class="row">
      <% @photos.each do |photo| %>
        <div class="col-xs-5 col-md-2 <%= "col-xs-offset-1" if @photos.index(photo) == 0 || @photos.index(photo) == 5 %>">
          <a href="#" class="thumbnail">
            <%= image_tag photo, id: "photo_#{@photos.index(photo)}" %>
          </a>
        </div>
      <% end %>
    </div>
  </div>
</div>